void setup() {
  Serial.begin(9600);  // Sets up serial communicaton at certain speed.
  oledDisplay.begin(SSD1306_SWITCHCAPVCC, 0x3C);  // Address 0x3C for 128x32
  oledDisplay.clearDisplay(); // Clears display so that the ad of Adafruit is not displayed

  // Displays setup video and text
  for (int index = 0; index < 9; ++index) {
    oledDisplay.setTextColor(WHITE);
    oledDisplay.setCursor(0, 0);
    oledDisplay.print(" Pulse Heart Detector");
    oledDisplay.drawBitmap(0, 5, setupVideoPhotos[index], 128, 64, WHITE);
    oledDisplay.display();
    delay(300);
    oledDisplay.clearDisplay();
  }

  // Setup pulse sensor module
  pulseSensor.analogInput(PULSE_WIRE);
  pulseSensor.setThreshold(THRESHOLD);
  pulseSensor.begin();
}

void loop() {
  // Displays ASCII heart symbol (♥) and BPM
  oledDisplay.setTextSize(2);
  oledDisplay.setTextColor(WHITE);
  oledDisplay.setCursor(116, 0);
  oledDisplay.write(3);
  oledDisplay.setCursor(0, 0);
  oledDisplay.write(3);
  oledDisplay.print(" BPM:");

  const int BPM = pulseSensor.getBeatsPerMinute();  // Calculates BPM (Calls function on our pulseSensor object that returns BPM as an "int")

  // When a beat is detected, the previous beat is cleared and the new beat is replaced. Nothing happens if no pulse is detected
  if (pulseSensor.sawStartOfBeat()) {
    oledDisplay.fillRect(70, 0, 40, 15, BLACK);
    oledDisplay.println(BPM);
  }
  oledDisplay.display();

  // Calculates logic of the axis of the heart rate line
  int sensorValue = analogRead(PULSE_WIRE);
  sensorValue = sensorValue > 530 ? 530 : sensorValue;
  sensorValue = sensorValue < THRESHOLD ? THRESHOLD : sensorValue;
  const int value = map(sensorValue, THRESHOLD, 530, 0, 44);

  // When the axis of the heartbeat line reaches the end of the screen on the X-axis, the screen is cleared
  const int y = 60 - value;
  if (x > 128) {
    x = 0;
    sX = 0;
    oledDisplay.clearDisplay();
  }

  // Displays the axis of the heart rate line
  oledDisplay.drawLine(sX, sY, x, y, WHITE);
  sX = x;
  sY = y;
  ++x;
}
